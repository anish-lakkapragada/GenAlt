{
  "manifest_version": 2,
  "name": "GenAlt",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/main.js"]
    }
  ], 
  "permissions": [
    "storage"
  ], 
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": ["describe.js"], 
  "browser_action": {
    "default_popup": "popup/public/index.html",
    "default_title": "GenAlt"
  }
}
